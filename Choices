#include<stdio.h>
int main()
{
	int i = 0;
	printf("Enter the choice\n 1.Pizza,Rs. 239\n 2.Burger,Rs 129\n 3. Pasta,Rs 179\n 4. French Fries,Rs 99\n 5. Sandwich,Rs 149 ");
	scanf("%d",&i);
	
	switch (i)
	{
		case 1:
			printf(" Food Item - Pizza\n Price - Rs 239");
			break;
			
		case 2:
			printf(" Food Item - Burger\n Price - Rs 129");
			break;
		case 3:
		printf(" Food Item - Pasta\n Price - Rs 179");
		break;
		
		case 4:
			printf(" Food Item - French Fries\n Price - Rs 99");
			break;
			
		case 5:
			printf(" Food Item - Sandwich\n Price - Rs 149");
			break;
			
		default : printf("Invalid choice");
		
	}
	return 0;
}
